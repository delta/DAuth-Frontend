		"typescript": "^4.5.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0"
	},
	"dependencies": {
		"@mdi/js": "^6.5.95",
		"serve": "^13.0.2"
	},
	"browserslist": [
		"defaults"
	],
	"husky": {
		"hooks": {
			"pre-commit": " echo 'staging' lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,css,md,svelte}": "prettier --write"
	}
}
