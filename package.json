{
	"name": "dauth-frontend",
	"version": "1.0.0",
	"description": "delta oauth2.0 frontend",
	"main": "main.ts",
	"private": true,
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack",
		"build:dev": "npm run validate && npm run prelint && npm run lint && cross-env NODE_ENV=development webpack",
		"dev": "webpack serve --config webpack.config.ts --mode development --host localhost",
		"start": "webpack serve --config webpack.config.ts --mode production --host localhost",
		"validate": "svelte-check",
		"format": "prettier --write ./src/**/*.{svelte,ts}",
		"lint": "eslint './src/**/*.{js,ts,svelte}'",
		"lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
		"prelint": "npm run format",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/delta/DAuth-Frontend.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/delta/DAuth-Frontend/issues"
	},
	"homepage": "https://github.com/delta/DAuth-Frontend#readme",
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/plugin-transform-runtime": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@babel/runtime": "^7.16.5",
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@mdi/js": "^6.5.95",
		"@tsconfig/svelte": "^2.0.1",
		"@types/css-minimizer-webpack-plugin": "^3.2.0",
		"@types/mini-css-extract-plugin": "^2.4.0",
		"@types/page": "^1.11.5",
		"@types/webpack-dev-server": "^4.5.0",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"autoprefixer": "^10.4.0",
		"axios": "^0.24.0",
		"babel-loader": "^8.2.3",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.20.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"dotenv-webpack": "^7.0.3",
		"eslint": "^8.4.1",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-svelte3": "^3.2.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^7.0.4",
		"intl-tel-input": "^17.0.15",
		"less-loader": "^10.2.0",
		"lint-staged": "^12.1.2",
		"mini-css-extract-plugin": "^2.4.5",
		"postcss-loader": "^6.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"regenerator-runtime": "^0.13.9",
		"sapper": "^0.29.3",
		"sass": "^1.45.0",
		"sass-loader": "^12.4.0",
		"sirv-cli": "^1.0.14",
		"style-loader": "^3.3.1",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.10",
		"svelte-loader": "^3.1.2",
		"svelte-materialify": "^0.3.11",
		"svelte-preprocess": "^4.10.0",
		"svelte-routing": "^1.6.0",
		"svelte-semantic-ui": "^1.0.1221",
		"svelte-toasts": "^1.1.2",
		"sveltestrap": "^5.6.3",
